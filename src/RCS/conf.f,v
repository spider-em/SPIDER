a35 1
C
d40 1
d42 10
a51 10
	CALL REDLIN(LUN1,BUF,NSAM,I)
	CALL REDLIN(LUN2,BUF(NSAM+1),NSAM,I)
	DO  J=1,NSAM
	AM=BUF(J)
	BM=ZALPHA*AN1*SQRT(BUF(NSAM+J))
	BUF(J)=AM+BM
	BUF(NSAM+J)=AM-BM
	ENDDO
	CALL WRTLIN(LUN3,BUF,NSAM,I)
	CALL WRTLIN(LUN4,BUF(NSAM+1),NSAM,I)
@


