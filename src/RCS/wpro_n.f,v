         DOUBLE PRECISION                 :: RAD_TO_DGR

         COMMON /PAR/      LDPX,LDPY,LDPZ
d65 2
a66 2
         CPHI = DCOS(DBLE(PHI)*DGR_TO_RAD)
         SPHI = DSIN(DBLE(PHI)*DGR_TO_RAD)
d69 2
a70 2
         CPSI = DCOS(DBLE(PSI)*DGR_TO_RAD)
         SPSI = DSIN(DBLE(PSI)*DGR_TO_RAD)
