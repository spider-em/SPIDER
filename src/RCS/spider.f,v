                 FUNCNU(IGO-1:IGO+1) = '***'
                 NCHAR = IGO+1
                 CALL FILGET(FUNCNU,FUNCNU,NCHAR,N1,IER)
                 IF (IER .NE. 0) THEN
C                   STOPS IN ERRT
                    CALL ERRT(101,
     &                  '#1B ERROR IN REGISTER SUBSTITUTION',NE)
                 ENDIF
              ENDIF
