C*******************************************************************
C*******************************************************************
C*******************************************************************
a978 4
3600	REWIND NUNIN
	CLOSE(NIN)
	CLOSE(NUNIN)
	OPEN(NIN,FILE=BNAME,STATUS='UNKNOWN')
d980 2
a981 7
 	IF (IDXTMP .GE. 1) THEN
	   DO I = 1, IDXTMP
            TMPARY(I:I) = '@@'
           ENDDO
        ENDIF
	IDXTMP=0
	GOTO 5000
d983 8
a990 1
C LIST BATCH FILE 
d992 1
