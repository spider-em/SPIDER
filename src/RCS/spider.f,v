     &                 NLIST1,NPARG)
	  IF (NLIST .NE. NLIST1) THEN
	     WRITE(NOUT,*) '*** ARGUMENTS TO PROCEDURE:',NLIST,
     &                     ' <> ARGUMENTS FROM:',NLIST1
d666 2
d669 3
