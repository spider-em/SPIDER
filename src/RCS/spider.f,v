@a161 7
C	FOR INLINED BUFFER BUFF.
C	LET SPLIT FBUF IN 2, CHANGE IT IN SETMODE ROUTINE.
	ISZBUF0 = ISZBUF
	INLNSIZE = ISZBUF / 2
	INLNAM1 = '___1' // NULL
	INLNAM2 = '___2' // NULL

