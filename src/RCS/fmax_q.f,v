	subroutine  fmax_q(x,map,n,xmaxo)
	dimension   x(n)
	logical*1  map(n)
d2 28
d31 5
a35 5
	do    i=1,n
	if(map(i))  xmax=amax1(xmax,x(i))
	enddo
	xmaxo=xmax
	end
@


