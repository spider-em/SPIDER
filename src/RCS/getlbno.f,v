1.5
log
@GPL License fixed
@
text
@d4 2
a5 1
C  GETLBNO.F -- 
d44 1
a44 1
        CHARACTER *(*) STRING
d46 2
a47 2
        CHARACTER *80 TEMP1
        LOGICAL       ISDIGI
d63 3
a73 22
C       DOES NOT CONVERT TO UPPERCASE (FOR SPEED)

	SUBROUTINE GETLBNOT(STRING,LBNO,IRTFLG)

        CHARACTER *(*) STRING

        LOGICAL       ISDIGI

        LBNO   = -1
        IRTFLG = 0

        IGO = INDEX(STRING,'LB')
        IF (IGO .LE. 0) RETURN

        NCHAR = 1
        IF (ISDIGI(STRING(IGO+3:IGO+3))) NCHAR = 2

        READ(STRING(IGO+2:IGO+NCHAR+1),8000) LBNO
8000    FORMAT(I6)

        RETURN
	END
@


