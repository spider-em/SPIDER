         IF (IRTFLG.NE.0) THEN 
            CALL ERRT(46,'SAQB_P, X',IER)
            DEALLOCATE (A,B,C,D)
            RETURN
         ENDIF 

         NSNR=LSD*NROW
         SOLD=8.0D0*DATAN(1.0D0)/360.0D0
d531 5
a535 5
         K1=1
         KT1=1
         KT2=1
         IMI=0
778      LERR=-1
d537 1
a537 1
         IF (LERR.NE.0)  GOTO  788
d539 2
