                   IF (IABS(IX - IXL) .GT. 1) THEN
d258 1
a258 1
                     IF (IX .LT. IXL) THEN
d264 2
a265 2
                         BUF((IYT -1) * NSAM + IXT) = FOREGR
                        IF (IXT .EQ. IHALF) IYT = IYEND                       
d285 3
