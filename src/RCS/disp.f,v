        INTEGER               :: ICOMM,MYPID,MPIERR, lnblnkn,NLETO
d60 5
a64 1
        CALL SET_MPI(ICOMM,MYPID,MPIERR) ! SETS ICOMM AND MYPID
d90 1
a90 1
     &          'IMAGEMAGICK DISPLAY OPTIONS (or <CR>) ',NULL,IRTFLG)
d99 99
@


