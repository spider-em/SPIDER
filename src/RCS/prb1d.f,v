@GPL_2010
@
text
@d27 8
a34 1
C IMAGE_PROCESSING_ROUTINE
a35 1
C        1         2         3         4         5         6         7
a38 2


d41 1
a41 1
        DOUBLE PRECISION B(NPOINT),C2,C3
d44 3
a46 2
        POS=0.0
        IF (NPOINT .EQ. 7) THEN
d49 1
d51 2
a52 1
        ELSEIF (NPOINT .EQ. 5) THEN
d55 1
d57 2
a58 1
        ELSEIF (NPOINT .EQ. 3) THEN
d61 2
a62 1
        ELSEIF (NPOINT .EQ. 9) THEN
d66 1
d69 1
d72 2
a73 2
        IF (C3.NE.0.0D0)  POS = C2/(2.*C3) - NHALF
        RETURN
@


