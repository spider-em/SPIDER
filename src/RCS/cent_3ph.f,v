C *  THE CENTER FOR LABORATORIES AND RESEARCH   			   *
d21 1
a21 1
         SUBROUTINE  CENT_3(LUN,B,X,Y,Z,NSAM,NROW,NSLICE,SNS,SNR,SNC)
d24 8
a31 9
         DO    I=1,NSAM
         X(I)=0.0
	 ENDDO
         DO    J=1,NROW
         Y(J)=0.0
	 ENDDO
         DO    K=1,NSLICE
         Z(K)=0.0
	 ENDDO
d33 8
a40 11
         DO    J=1,NROW
         CALL  REDLIN(LUN,B,NSAM,J+(K-1)*NROW)
         T=0.0
         DO    I=1,NSAM
         T=T+B(I)
         X(I)=X(I)+B(I)
	 ENDDO
         Y(J)=Y(J)+T
         Z(K)=Z(K)+T
	 ENDDO
	 ENDDO
d45 3
a47 3
         C=C+COS(P*(I-1))*X(I)
         S=S+SIN(P*(I-1))*X(I)
	 ENDDO
