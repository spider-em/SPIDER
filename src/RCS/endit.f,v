	SUBROUTINE ENDIT(INFLUN)
d23 5
d30 14
a43 1
        CHARACTER * 80 FCHAR
d45 1
a45 5
	CLOSE(INFLUN)
	CALL PDATES('COMPLETED ',-1)
	IF (FCHAR(4:4) .EQ. 'D') CLOSE(UNIT=NDAT,STATUS='DELETE')
	CLOSE(NDAT)
	CLOSE(NPROC)
@


