      CALL  TRIDI (NDIM,N,W,D,S)                                                
d41 2
a42 2
      T     = SQRT (Q*Q + 1.0)                                                  
      Q     = D(J) - H + S(K) / (Q + SIGN(T,Q))                                 
d53 1
a53 1
      T     = SQRT (V*V + 1.0)                                                  
d59 1
a59 1
      T     = SQRT (U*U + 1.0)                                                  
d101 1
