        DO  M  =  1, JcASE
          DO L  =  1, JcASE
d47 1
a47 1
            mc = 0
d49 3
a51 3
              if(inb(m))  then
                mc = mc + 1
                lc = 0
d53 2
a54 2
                  if(inb(l))  then
                    lc = lc + 1
d66 2
a67 2
                    AD(Lc, Mc)  =  AD(Lc, Mc)  +  PQ * CIL * CIM
                  endif
d69 1
a69 1
              endif
d74 1
