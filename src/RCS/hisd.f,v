90    FORMAT(' REQUESTED RANGE:     ',G10.3,'   .........   ',G10.3,/,
     &       ' HISTOGRAM RANGE:     ',G10.3,'   .........   ',G10.3,/,
     &       ' MODE:                ',G10.3,'   BIN SIZE:   ',G10.3)
