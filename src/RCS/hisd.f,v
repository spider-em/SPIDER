         B=BUF(ICOL)
               J = INT((B-FMIN)*FF) + 1.5
               IF (J.GE.1 .AND. J.LE.128)  THEN
                  BUF(ICOL+J) = BUF(ICOL+J)+1.
               ENDIF
         K=K+1
         GOTO  788
         ENDIF
