          DO  510  J=1,NFCNS
          DTEMP=0.0 
          DNUM=FLOAT(J-1)*DDEN
          IF(NM1.LT.1)   GO TO 505
          DO  500  K=1,NM1
 500          DTEMP=DTEMP+A(K+1)*DCOS(DNUM*FLOAT(K))
 505          DTEMP=2.0*DTEMP+A(1)
 510          ALPHA(J)=DTEMP
          DO  550  J=2,NFCNS
 550          ALPHA(J)=2.0*ALPHA(J)/CN
d243 4
a246 3
          DO  520  K=1,J
          A(K)=P(K) 
 520          P(K)=2.0*BB*A(K)
d249 3
a251 2
          DO  525  K=1,JM1
 525          P(K)=P(K)+Q(K)+AA*A(K+1)
d253 3
a255 2
          DO  530  K=3,JP1
 530          P(K)=P(K)+AA*A(K-1) 
d257 3
a259 2
          DO  535  K=1,J
 535          Q(K)=-A(K)
d263 3
