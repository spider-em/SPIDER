          DO  110  J=1,NZ 
d27 2
a28 1
 110          X(J)=DTEMP
d30 3
a32 2
          DO  120  J=1,NZ 
 120          AD(J)=DEE(J,NZ,JET) 
d36 8
a43 7
          DO  130  J=1,NZ 
          L=IEXT(J) 
          DTEMP=AD(J)*DES(L)
          DNUM=DNUM+DTEMP 
          DTEMP=FLOAT(K)*AD(J)/WT(L)
          DDEN=DDEN+DTEMP 
 130          K=-K
d49 6
a54 5
          DO  140  J=1,NZ 
          L=IEXT(J) 
          DTEMP=FLOAT(K)*DEVI/WT(L)
          Y(J)=DES(L)+DTEMP 
 140          K=-K
d167 5
