d122 16
a137 23
             A0   = F1(I2,J2,L2)
             A1   = X1(I2,J2,L2)
             B0   = F1(I3,J2,L2)
             B1   = X1(I3,J2,L2)

             A2   = 3*( B0-A0) - 2*A1 - B1
             A3   = 2*(-B0+A0) +   A1 + B1
             ADX  = A0 + A1*DX + A2*DX**2 + A3*DX**3

             A0   = F1(I2,J3,L2)
             A1   = X1(I2,J3,L2)
             B0   = F1(I3,J3,L2)
             B1   = X1(I3,J3,L2)
             A2   = 3*( B0-A0) - 2*A1 - B1
             A3   = 2*(-B0+A0) +   A1 + B1
