             A0   = Y1(I2,J2,L2)
             A1   = XY2(I2,J2,L2)
             B0   = Y1(I3,J2,L2)
             B1   = XY2(I3,J2,L2)
             A2   = 3*( B0-A0) -2*A1 - B1
             A3   = 2*(-B0+A0) +  A1 + B1
