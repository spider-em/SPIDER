	SUBROUTINE  CNRS_2(X,Y,O,NSAM,NROW)
	DIMENSION  X(*),Y(*),O(*)
d57 3
a59 2
	CALL  CNRS_2R(X,Y,O,(NSAM+2-MOD(NSAM,2))/2,NSAM,NROW)
	END
