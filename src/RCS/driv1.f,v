     &         RESPONSE(1:NCHAR) .NE. PROMPT(1:NLET2)) THEN
              WRITE(NOUT,*) '*** ERROR IN FILENAME FORMATION!!!'
              WRITE(NOUT,9065) RESPONSE(1:NCHAR),PROMPT(1:NLET2)
9065          FORMAT( '*** GOT: ',A,'  SHOULD BE: ',A/)
              CALL ERRT(100,' ',NE)
           ENDIF
        ENDIF
d162 1
