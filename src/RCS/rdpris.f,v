 1400 CALL READCH(PROMPT(:IDOL),NIN,NCHAR,ANSW,.TRUE.,IRTFLG)
a89 1
            IBCNT = IBCNT + 1
a97 1
            IBCNT = IBCNT - 1
d101 1
a101 3
 2300 IBCNT = IBCNT+1

      IF (NECHO .NE .0) WRITE(NECHO,2400) I1,I2
