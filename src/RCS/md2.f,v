1.5
log
@header
@
text
@d14 1
a14 1
C  MD2                                                                *
d20 2
d26 1
a26 1
	SUBROUTINE  MD2(X,Y,NSAM,NROW,L,A,K,MODE,LUNO)
d28 2
a29 2
	DIMENSION   X(NSAM,NROW),Y(NSAM),A(K)
	CHARACTER*1  MODE
d31 36
a66 36
	LH=L/2
	K21=K/2+1
	DO    J=1,NROW
	IF(MODE.EQ.'C')  THEN
	DO    I=1,NSAM
	LB=0
	DO  102  M=-LH,LH
	IF(M)  103,104,103
103	LB=LB+1
	A(LB)=X(I,MOD(J+M+NROW-1,NROW)+1)
	LB=LB+1
	A(LB)=X(MOD(I+M+NSAM-1,NSAM)+1,J)
	GOTO  102
104	LB=LB+1
	A(LB)=X(I,J)
102	CONTINUE
	CALL   FSORT(A,K)
	Y(I)=A(K21)
	ENDDO
	ELSE
	DO    I=1,NSAM
	LB=0
	DO    MJ=-LH,LH
	MJM=MOD(J+MJ+NROW-1,NROW)+1
	DO    MI=-LH,LH
	LB=LB+1
	A(LB)=X(MOD(I+MI+NSAM-1,NSAM)+1,MJM)
	ENDDO
	ENDDO
	CALL   FSORT(A,K)
	Y(I)=A(K21)
	ENDDO
	ENDIF
	CALL  WRTLIN(LUNO,Y,NSAM,J)
	ENDDO
	END	
@


