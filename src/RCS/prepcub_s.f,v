	SUBROUTINE  PREPCUB_S(N,NN,IPCUBE,RI)
d29 3
a31 3
	INTEGER  IPCUBE(5,*)
	LOGICAL  FIRST
	COMMON /PAR/  LDP,NM,LDPNM
d40 1
a40 1
	R=RI*RI
d42 2
a43 2
	NMAT=0
	NN=0
d45 37
a81 29
	DO    I1=1,N
	T=I1-LDP
	XX=T*T
	DO    I2=1,N
	T=I2-LDP
	YY=T*T+XX
	FIRST=.TRUE.
	DO    I3=1,N
	NMAT=NMAT+1
	T=I3-LDP
        RC=T*T+YY
	IF(FIRST)  THEN
	IF(RC-R)  80  ,80,14
80	FIRST=.FALSE.
	NN=NN+1
	IPCUBE(1,NN)=NMAT
	IPCUBE(2,NN)=NMAT
	IPCUBE(3,NN)=I3
	IPCUBE(4,NN)=I2
	IPCUBE(5,NN)=I1	
	ELSE
	IF(RC.LE.R)  IPCUBE(2,NN)=NMAT
	ENDIF
14	CONTINUE
	ENDDO
16	CONTINUE
	ENDDO
	ENDDO
	END
@


