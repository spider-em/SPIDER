      IF (FCHAR(IC+3:IC+4) .EQ. '-1') THEN
         MODE = -16
         CALL GETV16(FILOLD,FILNEW,LUNO,LUNN,MODE,IRTFLG)

      ELSEIF (FCHAR(IC+3:IC+3) .EQ. '1') THEN
         MODE = 16
         CALL GETV16(FILOLD,FILNEW,LUNO,LUNN,MODE,IRTFLG)

      ELSEIF (FCHAR(IC+3:IC+6) .EQ. 'RAW') THEN
