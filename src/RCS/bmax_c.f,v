	SUBROUTINE  BMAX_C(BCKE,NMAT,IPCUBE,NN,BMAX)
	DIMENSION    BCKE(NMAT),IPCUBE(5,NN)
d33 1
a33 1
		BMAX=AMAX1(BCKE(J),BMAX)
d36 2
a37 1
	END
@


1.4
log
@a
@
text
@a0 2
	subroutine  bmax_c(bcke,nmat,ipcube,nn,bmax)
	dimension    bcke(nmat),ipcube(5,nn)
d2 33
a34 7
		bmax=bcke(ipcube(1,1))
		do    i=1,nn
		do    j=ipcube(1,i),ipcube(2,i)
		bmax=amax1(bcke(j),bmax)
		enddo
		enddo
	end
@


