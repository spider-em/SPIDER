      IF (MAXBIN .EQ. 1) THEN
d171 1
a171 1
      ELSEIF (MAXBIN .EQ. NBINS) THEN
d190 1
a190 1
      ELSEIF (FNUMEL .EQ. 1) THEN
d198 1
a198 1
      FNPIX  = NSAM * NROW * NSLICE
d205 1
a205 1
         NPIX  = NSAM * NROW * NSLICE
