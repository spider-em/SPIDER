           WIAVG = 0.0
           DO  J = 1, NIMA
              WIAVG = WIAVG + WI(J)
           ENDDO
           WIAVG = WIAVG / FLOAT(NIMA)
d519 1
a519 1
           DO L = 1, NF
d541 1
a541 1
C  SRIP2(NIMA,NPIX,NFAC,LUNI,LUNP,LUND,CI,WI,IDI,CP,WP,IDP,D)
