1.8
log
@change wasn't very f77 compatible, fixed it
@
text
@d27 1
a27 1
	INTEGER*2  NUMR(3,NRING),MAXRIN,CMP
d35 1
a35 2
	IP=MAXRIN
	IP=-LOG2_P(IP)
