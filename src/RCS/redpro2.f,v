	klin=klin+klp
	abin=(abin+aba)/klin
	aba=aba/klp
	ltb =nsam*nang
	ltbn=nsam*nang
d71 1
a71 1
	write(nout,1111)  klin,AMI,AMA,Abin,aba
@


