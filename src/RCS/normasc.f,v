C DETERMINE NORMALIZATION PARAMETERS BY VARIANCE
d43 3
a45 1
        SUBROUTINE NORMASC(X,NS1,NS2,NR1,NR2,IR1,IR2, AVT,VRINV)
d48 1
a48 1
        INTEGER, INTENT(IN)           :: IR1,IR2
d50 1
d52 2
a53 2
        DOUBLE PRECISION              :: VR,DTEMP
        INTEGER                       :: N
d55 3
a57 3
        R1SQ = IR1 * IR1
        R2SQ = IR2 * IR2
        AVT  = 0.0
d61 26
a86 8
        DO J=NR1,NR2
           DO I=NS1,NS2
              R = FLOAT(J*J + I*I)
              IF (R .GE. R1SQ .AND. R .LE. R2SQ)  THEN
                 N  = N + 1
                 AVT = AVT + X(I,J)
                 VR  = VR + X(I,J ) * X(I,J)
              ENDIF
d88 1
a88 1
        ENDDO
d90 1
a90 1
        AVT   = AVT / N
@


