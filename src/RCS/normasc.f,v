1.6
log
@zero div. trap
@
text
@d7 1
d36 1
a36 1
C SERIALLY NORMALIZES X --> Y BY VARIANCE
d43 1
a43 1
        SUBROUTINE  NORMASC(X,Y,NS1,NS2,NR1,NR2,IR1,IR2)
d45 6
a50 3
        REAL    :: X(NS1:NS2,NR1:NR2),Y(NS1:NS2,NR1:NR2)
        INTEGER :: IR1,IR2
        REAL*8  :: AV,VR
d54 1
a54 1
        AV   = 0.0
d63 2
a64 2
                 AV = AV + X(I,J)
                 VR = VR + X(I,J )* X(I,J)
d69 2
a70 2
        AV    = AV / N
        DTEMP = (VR - N * AV * AV)
d72 1
a75 1
           Y     = (X - AV) * VRINV     ! ARRAY OPERATION ON Y
d78 1
a78 1
           Y = 0.0    ! ARRAY OPERATION ON Y
a80 2
C       MULTIPLICATION IS FASTER

@


1.5
log
@GPL License fixed
@
text
@d5 2
a6 1
C                         SPEED-UP                  OCT 01 ARDEAN LEITH
d9 1
a9 1
C=* Copyright (C) 1985-2005  Health Research Inc.                      *
d33 5
a37 1
C IMAGE_PROCESSING_ROUTINE
d44 3
a46 3
        DIMENSION  X(NS1:NS2,NR1:NR2),Y(NS1:NS2,NR1:NR2)
        INTEGER    IR1,IR2
        REAL*8     AV,VR
d65 12
a76 1
        AV = AV / N
a77 3
        VR = 1.0 / (DSQRT((VR-N*AV*AV) / (N-1)))

        Y = (X - AV) * VR
@


