d212 1
a216 2
                 !write(6,*) ' fsb:',nsam1,nrow1,' -->',nsam2,nrow2,scalex

d219 1
a219 22
     &                               NSAM2,NROW2,  
     &                           SCALEX, SCALEY,IRTFLG)
              ELSE
                  CALL ERRT(101,'VOLUME INTERP. NOT IMPLEMENTED',NE)
              ENDIF 

C             X2F IS NOT FOURIER PADDED!
              CALL WRITEV(LUN2,X2F, NSAM2,NROW2, NSAM2,NROW2,NSLICE2)

           ELSEIF (FCHAR(4:5) .EQ. 'FP') THEN
C             FOURIER BASED POLYNOMIAL INTERPOLATION -------------

              CALL READV(LUN1,X1F, LSD,NROW1, NSAM1,NROW1,NSLICE1)

              IF (NSLICE1 .EQ. 1) THEN
C                X2F IS NOT FOURIER PADDED
                 !write(6,*) ' fsb:',nsam1,nrow1,' -->',nsam2,nrow2,scalex

                 CALL INTERP_FBP(X1F, X2F, 
     &                           LSD,NSAM1,NROW1,
     &                               NSAM2,NROW2,  
     &                           SCALEX, SCALEY,IRTFLG)
d221 2
a222 1
                  CALL ERRT(101,'VOLUME INTERP. NOT IMPLEMENTED',NE)
a227 1
 
d231 2
d238 2
