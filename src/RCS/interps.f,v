           NSLICE2 = 1
           CALL RDPRIS(NSAM2,NROW2,NOT_USED,
     &                'ENTER DIMENSIONS, NSAM & NROW',IRTFLG)
d135 2
a136 2
        IF (NROW2 .EQ. 0) THEN
           NROW2 = (FLOAT(NSAM2) / FLOAT(NSAM1)) * FLOAT(NROW1)
d144 1
