        ELSEIF (FCHAR(4:5) .EQ. 'FS' .AND. NSLICE1 > 1) THEN
C          FOURIER BASED SPLINE INTERPOLATION
           CALL ERRT(101,'OPERATION DOES NOT WORK ON VOLUMES',NE)
           GOTO 9999
d196 1
