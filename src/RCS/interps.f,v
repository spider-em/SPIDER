        IF (FCHAR(4:4) == 'T' .AND. NSLICE1 > 1) THEN
d108 6
a113 6
        NSAM2 = 0
        NROW2 = 0
        IF (NSLICE1 > 1) THEN
           NSLICE2 = -1
           CALL RDPRI3S(NSAM2,NROW2,NSLICE2,NOT_USED,
     &                'ENTER DIMENSIONS, NSAM, NROW, & NSLICE',IRTFLG)
