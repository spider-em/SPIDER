     &             IFORM1,NSAM2,NROW2,NSLICE2,MAXIM2,
d154 3
a156 3
        SCALEX = FLOAT(NSAM1)   / FLOAT(NSAM2)
        SCALEY = FLOAT(NROW1)   / FLOAT(NROW2)
        SCALEZ = FLOAT(NSLICE1) / FLOAT(NSLICE2)
d161 1
a161 1
        IF (NSLICE1 > 1 .AND. 
d164 2
