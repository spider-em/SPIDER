     &                           LSD,NSAM1,NROW1,
     &                               NSAM2,NROW2,  IRTFLG)
d211 2
a212 2
                 CALL INTERP_FBS3(X1F,X2F,NSAM1,NROW1,NSLICE1,
     &                                    NSAM2,NROW2,NSLICE2,  LSD)
d216 1
a216 1
              CALL WRITEV(LUN2,X2F, NSAM2,NROW2, NSAM2,NROW2,NSLICE2)
d223 1
a223 1
              CALL TRINTER(LUN1,LUN2,NSAM1,NROW1,1,NSAM2,NROW2,1)
d230 2
a231 2
              IF (NSLICE1 .EQ. 1)  THEN
                 CALL INTERP(LUN1,LUN2,NSAM1,NROW1,NSAM2,NROW2,MAXDIM)
d233 2
a234 2
                 CALL INTERP3(LUN1,LUN2,NSAM1,NROW1,NSLICE1,
     &                        NSAM2,NROW2,NSLICE2,IDUM)
