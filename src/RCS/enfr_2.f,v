	DO  71  J=1,NROW
	DO  71  I=3,LSD-ISBD
71	ENFR=ENFR+A(I,J)*DBLE(A(I,J))
	DO  72  J=2,NROW/2+IRBD
72	ENFR=ENFR+A(1,J)*DBLE(A(1,J))+A(2,J)*DBLE(A(2,J))
d40 3
a42 2
	DO  73  J=2,NROW/2+IRBD
