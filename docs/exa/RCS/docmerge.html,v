<table class="opl">
  <tr> <div class="oplin">  <td class="op">.OPERATION:                        </td> <td class="opres">DOC MERGE      </td> <td class="com">; &nbsp; Merge two document files</td> </div> </tr> 
  <tr> <div class="doc_in"> <td class="pr">.FIRST DOCUMENT FILE:              </td> <td class="res">  sav_docmerge_0 </td> <td class="com">; &nbsp; First doc file    (input)    </td> </div> </tr>
  <tr> <div class="doc_in"> <td class="pr">.SECOND DOCUMENT FILE:             </td> <td class="res">  sav_docmerge_1 </td> <td class="com">; &nbsp; Second doc file   (input)      </td> </div> </tr>
  <tr> <div class="doc_out"><td class="pr">.OUTPUT DOCUMENT FILE:             </td> <td class="res">  docmerge_doc   </td> <td class="com">; &nbsp; Output doc file   (output)       </td> </div> </tr>
  <tr> <div class="n">      <td class="pr">.COLUMN TO BE MERGED BY (0 IS KEY):</td> <td class="res">  0              </td> <td class="com">; &nbsp; Merge by keys           </td> </div> </tr>
</table>
